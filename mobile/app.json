{
  "expo": {
    "name": "Atlas",
    "slug": "atlas",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2D2D2D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tristanokeefe.atlas"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2D2D2D"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tristanokeefe.atlas"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dd6c586e-249c-4d4b-85f5-170c88a86b5d"
      }
    },
    "owner": "okeetris05"
  }
}
